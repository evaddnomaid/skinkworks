<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tictactoe_egg SYSTEM "tictactoe_egg.dtd">
<tictactoe_egg>
    <egg>
        <board>
            <row><cell id="A1" player="">X</cell><cell id="A2" player=""></cell><cell id="A3" player="">X</cell></row>
            <row><cell id="B1" player="">X</cell><cell id="B2" player="">O</cell><cell id="B3" player="">X</cell></row>
            <row><cell id="C1" player="">O</cell><cell id="C2" player="">X</cell><cell id="C3" player="">O</cell></row>
        </board>
    </egg>
    <grub>
        <move>
            <player fill_regex="/^[XO]$/">X</player>
            <position fill_regex="/^[ABC][123]$/">A1</position>
        </move>
    </grub>
</tictactoe_egg>
